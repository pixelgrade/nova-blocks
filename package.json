{
	"name": "nova-blocks",
	"version": "1.5.3",
	"main": "blocks/index.js",
	"license": "GPL-3.0+",
	"scripts": {
		"clean:packages": "rimraf \"./packages/*/@(build|build-module|build-style)\"",
		"prebuild:packages": "npm run clean:packages",
		"build": "npm run build:packages && wp-scripts build",
		"build:packages": "node ./bin/packages/build.js",
		"dev": "npm run build:packages && concurrently \"wp-scripts start\" \"npm run dev:packages\"",
		"dev:packages": "node ./bin/packages/watch.js"
	},
	"dependencies": {
		"@novablocks/block-library": "file:packages/block-library",
		"@novablocks/components": "file:packages/components",
		"@novablocks/core": "file:packages/core",
		"@novablocks/easings": "file:packages/easings",
		"@novablocks/icons": "file:packages/icons",
		"@novablocks/utils": "file:packages/utils",
		"@wordpress/blocks": "^6.22.0",
		"@wordpress/date": "^3.11.1",
		"@wordpress/element": "^2.17.1",
		"@wordpress/i18n": "^3.15.0",
		"@wordpress/url": "^2.18.0",
		"js-cookie": "^2.2.1"
	},
	"devDependencies": {
		"@babel/plugin-proposal-optional-chaining": "^7.10.4",
		"@wordpress/babel-preset-default": "^4.12.1",
		"@wordpress/custom-templated-path-webpack-plugin": "^1.6.0",
		"@wordpress/dependency-extraction-webpack-plugin": "^2.2.0",
		"@wordpress/library-export-default-webpack-plugin": "^1.6.0",
		"@wordpress/scripts": "^3.4.0",
		"autoprefixer": "^9.7.4",
		"babel-plugin-inline-json-import": "^0.3.2",
		"concurrently": "^5.3.0",
		"copy-webpack-plugin": "^5.1.1",
		"cssnano": "^4.1.10",
		"fast-glob": "^3.1.1",
		"loadsh": "0.0.4",
		"node-sass": "^4.13.1",
		"node-watch": "^0.6.4",
		"path": "^0.12.7",
		"postcss": "^7.0.27",
		"rimraf": "^3.0.2",
		"rtlcss": "^2.5.0",
		"terser-webpack-plugin": "^4.2.2",
		"webpack": "4.41.0",
		"webpack-cli": "^3.3.12"
	}
}
